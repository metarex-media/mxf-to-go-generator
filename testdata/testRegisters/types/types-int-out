package mxf2go

import (
    "encoding/binary"
	"unicode/utf16"
	"unicode/utf8"
)


func EncodeTUTF16StringArray(value TUTF16StringArray) ([]byte, error) {
	parts := utf16.Encode([]rune(string(value)))
	result, _ := EncodeTUInt16Array([]uint16(parts))
	return result, nil
}